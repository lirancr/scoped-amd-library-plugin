{
	"semi": false,
	"trailingComma": "es5",
	"singleQuote": true,
	"printWidth": 120,
	"tabWidth": 4,
	"useTabs": true,
	"overrides": [
		{
			"files": ["package.json", "tsconfig.json"],
			"options": {
				"useTabs": false,
				"tabWidth": 2
			}
		}
	],
	"quoteProps": "as-needed",
	"bracketSpacing": true,
	"arrowParens": "always",
	"proseWrap": "preserve",
	"htmlWhitespaceSensitivity": "css",
	"endOfLine": "auto"
}
